{
  "name": "msbios/media-doctrine",
  "description": "MSBios Media Doctrine",
  "license": "MIT",
  "keywords": [
    "php"
  ],
  "homepage": "https://github.com/msbios/msbios-media-doctrine",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Judzhin Miles",
      "email": "info@msbios.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MSBios\\Media\\Doctrine\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MSBiosTest\\Media\\Doctrine\\": "tests/"
    }
  },
  "require": {
  },
  "require-dev": {
    "msbios/portal-doctrine": "^1.0",
    "msbios/debug": "^1.0",
    "msbios/test": "^1.0"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": [
      "@cs-check",
      "phpunit --colors=always"
    ],
    "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
    "upload-coverage": "coveralls -v",
    "alter": "./vendor/bin/doctrine-module orm:schema-tool:update --force",
    "serve": "php -S 0.0.0.0:3107 -t public public/index.php",
    "serve-ios": "php -S 0.0.0.0:3107 -t public index.php",
    "commit": "git add . && git commit -a -m \"Lazy Composer Commiter\" && git push"
  },
  "extra": {
    "zf": {
      "component": "MSBios\\Media\\Doctrine"
    }
  }
}
